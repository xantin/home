[user]

	email = david.dutkovsky@gmail.com
	name = David Dutkovsky
[alias]
	co = checkout
	ci = commit
  	st = status
  	br = branch
  	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  	type = cat-file -t
  	dump = cat-file -p
	lo = log --oneline
	lon = "log --oneline -n"
	df = diff
	dfc = diff --cached
	s = show
	fic = 'diff-tree --no-commit-id --name-only -r'
[push]
	default = current
[core]
	eol = lf
	autocrlf = input
[merge]
	ff = only
	tool = kdiff3
	tool = kdiff3git
[color]
	ui = auto
